#include <iostream>
#include <string>
#include <cctype>
using namespace std;


class Stack {
private:
int top;
int arr[100];
int size;


public:
Stack() {
top = -1;
size = 100;
}


bool isEmpty() {
return top == -1;
}


bool isFull() {
return top == size - 1;
}


void push(int value) {
if (isFull()) {
cout << "Error: Stack Overflow" << endl;
return;
}
arr[++top] = value;
}


int pop() {
if (isEmpty()) {
cout << "Error: Stack Underflow" << endl;
return -1;
}
return arr[top--];
}


int peek() {
if (isEmpty()) {
cout << "Error: Stack Empty" << endl;
return -1;
}
return arr[top];
}
};


int main() {
string infix;
cout << "Enter infix expression: ";
getline(cin, infix);


string postfix = infixToPostfix(infix);


if (postfix.rfind("Error", 0) == 0) {
cout << postfix << endl;
return 0;
}


cout << "Postfix: " << postfix << endl;


int result = evaluatePostfix(postfix);


if (result != -1)
cout << "Result: " << result << endl;


return 0;
}
