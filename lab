#include <iostream>
using namespace std ;

#define size 6 
class stack{

public:    
int arr[size] ;

stack{
int top = -1 ;

}

bool is_full(){
if (top >= size -1){
    return true ;
}
    
else{
    return false ;
}
bool is_empty(){
if (top == -1){
    return true ;
}
else {
    return false ;
}
}
}
void push (int x) {
if ( is_full ){
   cout <<"stack is Full" ;
}
else{
arr [++top] = x ;
//return arr[top];
}


}
int  pop () {

    if (is_empty){
        return -1 ;
    }
else {
    cout << arr[top--] ;

}
}

void print_topvalue (){
cout << arr[top];
}

}
