#include <iostream>
#include <string>
#include <set> 

using namespace std;

int main() {
    string username;
    cin >> username;

    set<char> distinctLetters;

    
    for (char c : username) {
        distinctLetters.insert(c);
    }

    
    if (distinctLetters.size() % 2 == 0) {
        cout << "chat with her" << endl;
    } else {
        cout << "ignore him" << endl;
    }

    return 0;
}
